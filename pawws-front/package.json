{
  "name": "Pawws-v2",
  "version": "1.0.0",
  "private": true,
  "description": "Pawws Single Page App UI",
  "dependencies": {
    "all": "0.0.0",
    "history": "~4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.0.0",
    "chakram": "~1.5.0",
    "css-loader": "~0.28.7",
    "eslint": "~4.16.0",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-chai-friendly": "~0.4.0",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "~4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "~1.1.5",
    "fs-extra": "^5.0.0",
    "jsdom": "~11.7.0",
    "mocha": "^5.2.0",
    "mustache-loader": "^1.2.0",
    "node-sass": "^4.9.0",
    "nyc": "^12.0.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "prop-types": "^15.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "style-loader": "~0.20.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "uglify-js": "^2.8.0",
    "url-loader": "~0.6.2",
    "watch": "^1.0.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "~3.1.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "~3.1.10",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "api-test": "mocha --timeout 30000000 test/apitest",
    "build": "webpack --config ./webpack.config.js --mode production",
    "build:simulate": "npm run build && http-server dist",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "critter": "npm install && npm run stylelint:prod && npm run coverage && npm run eslint:prod && npm start",
    "critter:jenkins": "npm install && npm run stylelint:prod && npm run coverage && npm run eslint:prod",
    "ui-test": "cross-env ENV=local wdio test/e2e/wdio.local.conf.js",
    "ui-test:all": "cross-env ENV=local wdio test/e2e/wdio.allTests.conf.js",
    "ui-test:dev": "cross-env ENV=dev wdio test/e2e/wdio.dev.conf.js",
    "package": "cross-env NODE_ENV=production babel-node tools/run package",
    "eslint": "npm run eslint:prod -- --fix",
    "eslint:prod": "eslint --ext .json,.jsx,.js src tools wdio*",
    "eslint:watch": "watch 'npm run eslint' src/scripts",
    "coverage": "nyc -n src -c false --ignore-class-method render --check-coverage --branches 100 --statements 100 --lines 100 --functions 100 -e .jsx npm run test",
    "coverage:watch": "watch 'npm run coverage' src/scripts",
    "coverage:report": "nyc -n src -c false --ignore-class-method render --check-coverage -e .jsx npm run test && nyc report --reporter=html",
    "stylelint": "npm run stylelint:prod -- --fix",
    "stylelint:prod": "stylelint \"src/**/*.scss\"",
    "test": "mocha src/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}
